{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "User",
    "additionalProperties": false,
    "properties": {
        "gender": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "avatarUrl": {
            "type": "string"
        },
        "username": {
            "type": "string"
        },
        "dob": {
            "type": "string"
        },
        "phone": {
            "type": "string"
        },
        "botIds": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "location": {
            "type": "object",
            "properties": {
                "street": {
                    "type": "string"
                },
                "city": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "postcode": true
            }
        },
        "level": {
            "enum": [
                0,
                50
            ],
            "type": "integer"
        }
    },
    "required": [
        "username",
        "email"
    ],
    "type": "object"
}
